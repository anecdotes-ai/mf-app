{
  "version": 2,
  "projects": {
    "platform": {
      "projectType": "application",
      "root": "apps/platform",
      "sourceRoot": "apps/platform/src",
      "prefix": "mf-app",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:webpack-browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/platform",
            "index": "apps/platform/src/index.html",
            "main": "apps/platform/src/main.ts",
            "polyfills": "apps/platform/src/polyfills.ts",
            "tsConfig": "apps/platform/tsconfig.app.json",
            "assets": [
              "apps/platform/src/favicon.ico",
              "apps/platform/src/assets",
              {
                "glob": "**/*",
                "input": "./libs/shared/assets",
                "output": "assets"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@pdftron/webviewer/public",
                "output": "assets/webviewer"
              }
            ],
            "styles": ["apps/platform/src/styles.css"],
            "scripts": [],
            "customWebpackConfig": {
              "path": "apps/platform/webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/platform/src/environments/environment.ts",
                  "with": "apps/platform/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "customWebpackConfig": {
                "path": "apps/platform/webpack.prod.config.js"
              }
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@nrwl/angular:webpack-server",
          "configurations": {
            "production": {
              "browserTarget": "platform:build:production"
            },
            "development": {
              "browserTarget": "platform:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 3600
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "platform:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/platform/src/**/*.ts",
              "apps/platform/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/platform"],
          "options": {
            "jestConfig": "apps/platform/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-assets": {
      "projectType": "library",
      "root": "libs/shared/assets",
      "sourceRoot": "libs/shared/assets"
    },
    "shared-core": {
      "projectType": "library",
      "root": "libs/shared/core",
      "sourceRoot": "libs/shared/core/src",
      "prefix": "mf-app",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/core"],
          "options": {
            "jestConfig": "libs/shared/core/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/core/src/**/*.ts",
              "libs/shared/core/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "shared-modules": {
      "projectType": "library",
      "root": "libs/shared/modules",
      "sourceRoot": "libs/shared/modules/src",
      "prefix": "mf-app",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/modules"],
          "options": {
            "jestConfig": "libs/shared/modules/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/modules/src/**/*.ts",
              "libs/shared/modules/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "shell": {
      "projectType": "application",
      "root": "apps/shell",
      "sourceRoot": "apps/shell/src",
      "prefix": "mf-app",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:webpack-browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/shell",
            "index": "apps/shell/src/index.html",
            "main": "apps/shell/src/main.ts",
            "polyfills": "apps/shell/src/polyfills.ts",
            "tsConfig": "apps/shell/tsconfig.app.json",
            "assets": [
              "apps/shell/src/favicon.ico",
              "apps/shell/src/assets",
              {
                "glob": "**/*",
                "input": "./libs/shared/assets",
                "output": "assets"
              }
            ],
            "styles": ["apps/shell/src/styles.css"],
            "scripts": [],
            "customWebpackConfig": {
              "path": "apps/shell/webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "deployUrl": "/apps/shell/",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/shell/src/environments/environment.ts",
                  "with": "apps/shell/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "customWebpackConfig": {
                "path": "apps/shell/webpack.prod.config.js"
              }
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@nrwl/angular:webpack-server",
          "configurations": {
            "production": {
              "browserTarget": "shell:build:production"
            },
            "development": {
              "browserTarget": "shell:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 3000
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "shell:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/shell/src/**/*.ts",
              "apps/shell/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/shell"],
          "options": {
            "jestConfig": "apps/shell/jest.config.js",
            "passWithNoTests": true
          }
        },
        "serve-mfe": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["nx serve shell", "nx serve platform"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "shell",
  "generators": {
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  }
}
